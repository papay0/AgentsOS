# AgentsOS Environment Variables
# Add your actual API keys here

# Firebase Configuration
# Get these from your Firebase project settings
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Clerk
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your-publishable-key
CLERK_SECRET_KEY=your-secret-key

# Firebase Admin SDK
FIREBASE_PRIVATE_KEY=""
FIREBASE_CLIENT_EMAIL=""

# Encryption (generate with: openssl rand -base64 32)
ENCRYPTION_SECRET=your_32_char_encryption_secret_here

# API URL Configuration
# Use your domain for production or local IP for development
AGENTSOS_API_URL=https://your-domain.com

# WebSocket Proxy URL (for terminal connections)
# Uncomment the one you want to use:

# LOCAL DEVELOPMENT (comment out for production)
NEXT_PUBLIC_WEBSOCKET_PROXY_URL=ws://localhost:3000

# PRODUCTION TESTING - Railway URL (comment out for local)
# NEXT_PUBLIC_WEBSOCKET_PROXY_URL=wss://agentsos-proxy-production.up.railway.app

# PRODUCTION CUSTOM DOMAIN (comment out for local/testing)  
# NEXT_PUBLIC_WEBSOCKET_PROXY_URL=wss://proxy.agentsos.app

# HTTP Proxy Domain for subdomain port access
NEXT_PUBLIC_HTTP_PROXY_DOMAIN=agentspod.dev

# Cloudflare Tunnel Token (optional, for authenticated tunnels)
# CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token_here

# Daytona API Configuration
# Set to your own Daytona API key for development
# In production, users provide their own keys via onboarding
DAYTONA_API_KEY=your_daytona_api_key_here

# Feature flags
# Set to true to require users to provide their own API keys
REQUIRE_USER_API_KEY=false